{
  "envId": "{{envId}}",
  "functionRoot": "cloudfunctions",
  "functions": [
    {
      "name": "nuxt-ssr-echo",
      "config": {
        "timeout": 5,
        "envVariables": {},
        "runtime": "Nodejs10.15",
        "memorySize": 128
      }
    }
  ],
  "framework": {
    "name": "nuxt-ssr",
    "plugins": {
      "client": {
        "use": "@cloudbase/framework-plugin-nuxt",
        "inputs": {}
      },
      "server": {
        "use": "@cloudbase/framework-plugin-function",
        "inputs": {
          "outputPath": "functionRootPath"
        }
      }
    }
  }
}
