{
  "envId": "{{envId}}",
  "functionRoot": "./functions",
  "functions": [
    {
      "name": "app",
      "timeout": 5,
      "envVariables": {},
      "runtime": "Java8",
      "triggers": [
        {
          "name": "myTrigger",
          "type": "timer",
          "config": "0 0 2 1 * * *"
        }
      ],
      "params": {},
      "handler": "index.main"
    }
  ]
}