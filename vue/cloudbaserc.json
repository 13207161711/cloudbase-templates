{
  "envId": "{{envId}}",
  "functionRoot": "cloudfunctions",
  "functions": [
    {
      "name": "helloworld",
      "config": {
        "timeout": 5,
        "envVariables": {},
        "runtime": "Nodejs10.15",
        "memorySize": 128
      }
    }
  ],
  "framework": {
    "plugins": {
      "vue": {
        "use": "@cloudbase/framework-plugin-website",
        "inputs": {
          "buildCommand": "npm run build",
          "outputPath": "dist"
        }
      },
      "function": {
        "use": "@cloudbase/framework-plugin-function",
        "inputs": {
          "outputPath": "cloudfunctions"
        }
      }
    }
  }
}
